{
    "name": "justasb/phpunit-mock-api",
    "description": "A phpunit helper library for mocking API calls.",
    "type": "library",
    "authors": [
        {
            "name": "Justas Bieliauskas",
            "email": "justyvas0@gmail.com"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.4",
        "phpunit/phpunit": "^9.5",
        "psr/http-message": "^1.0"
    },
    "require-dev": {
        "guzzlehttp/guzzle": "^7.4"
    },
    "autoload": {
        "psr-4": {
            "Justasb\\MockApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Justasb\\MockApi\\Test\\": "tests/"
        }
    }
}
